    "@opentelemetry/instrumentation-express": "^0.33.2",
    "@opentelemetry/instrumentation-http": "^0.45.2",
    "@opentelemetry/instrumentation-ioredis": "^0.33.2",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/sdk-metrics": "^1.18.1",
    "@opentelemetry/sdk-node": "^0.45.2",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
